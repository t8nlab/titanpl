{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@titan/native": [
        "app/t.native"
      ],
      "@titan/route": [
        "./titan/titan"
      ],
      "*": [
        "./app/*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "titan/**/*",
    "node_modules/**/titan-ext.d.ts"
  ]
}